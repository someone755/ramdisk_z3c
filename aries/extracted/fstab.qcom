/dev/block/platform/msm_sdcc.1/by-name/system   /system      ext4 ro,barrier=1                                           wait,verify=/dev/block/mmcblk0p23
/dev/block/platform/msm_sdcc.1/by-name/userdata /data        ext4 noatime,nosuid,nodev,barrier=1,noauto_da_alloc,discard wait,check,encryptable=footer
/dev/block/platform/msm_sdcc.1/by-name/cache    /cache       ext4 noatime,nosuid,nodev,barrier=1,discard                 wait,check,formattable
/devices/msm_sdcc.2/mmc_host*                   auto         auto defaults                                               voldmanaged=sdcard1:auto,encryptable=userdata
/dev/block/platform/msm_sdcc.1/by-name/apps_log /idd         ext4 nosuid,nodev,noatime,noexec,barrier=1,discard          wait,check
/devices/platform/xhci-hcd/usb*                 auto         auto defaults                                               voldmanaged=usbdisk:auto
/dev/block/platform/msm_sdcc.1/by-name/misc     /misc        emmc defaults                                               defaults
/dev/block/platform/msm_sdcc.1/by-name/recovery /recovery    emmc defaults defaults
